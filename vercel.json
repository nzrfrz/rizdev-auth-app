{
    "version": 2,
    "builds": [
      {
        "src": "dist/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/app.js"
      },
      {
        "src": "/",
        "dest": "client/index.html"
      }
    ]
}